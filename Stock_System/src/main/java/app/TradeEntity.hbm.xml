<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="app.TradeEntity" table="trade" schema="stock_system">
        <id name="tradedate" column="tradedate"/>
        <property name="tradecode" column="tradecode"/>
        <property name="tradename" column="tradename"/>
        <property name="tradeprice" column="tradeprice"/>
        <property name="tradenumber" column="tradenumber"/>
        <property name="tradetypeid" column="tradetypeid"/>
        <property name="usersid" column="usersid"/>
        <property name="cheak" column="cheak"/>
    </class>
</hibernate-mapping>